<!DOCTYPE html>
<html lang="en">
  <head>
    <% include ../static/partials/head.ejs %>
  </head>

  <body style="background-color: #FFEAD0;">

    <% include ../static/partials/navbar1.ejs %>
    
    <div class="main">
      <div class="text-center">
        <h3 id="title" class="display-1" style="color: #37505C; font-family: URW Chancery; font-size: 68px;">Please Sign In</h3>
      </div>
    </div>
    <br>
    
    <div class="row">
      <div class="col-md-12">                 
        <% include ../static/partials/navbar2.ejs %>
      </div>
    </div>
<br>
<br>
<form action="/users/signIn" method="post">
    <div class="row text-center" style="color: #F76F8E;">
      <div class ="col-md-4"></div>
      
      <div class="col-md-4">
        
          <ul class="navbar-nav ml-auto text-center">
            <li>
              <label for="email"></label>
              <input type="text" name="email" placeholder="Email">
            </li>
            <li> 
              <label for="password"></label>
              <input type="text" name="password" placeholder="Enter Your Password">
            </li>
            <br>
            <li class="text-center">
              <input type="submit" value="Sign In" class="btn btn-primary" />
            </li>
        
          </ul>    
        
      </div>

  </div>
</form>
<% include ../static/partials/messages.ejs %>
  </body>
</html>




